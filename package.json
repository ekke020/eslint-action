{
  "name": "eslint-action",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main.ts",
  "scripts": {
    "build": "npm run ncc && npm run move-js-yaml && npm run replace-text",
    "ncc": "ncc build src/main.ts -o dist/",
    "move-js-yaml": "cp -R node_modules/js-yaml/ dist/js-yaml",
    "replace-text": "sed -i 's/js-yaml\/\\.\/js-yaml/g' dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ekke020/eslint-action.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ekke020/eslint-action/issues"
  },
  "homepage": "https://github.com/ekke020/eslint-action#readme",
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vercel/ncc": "^0.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "copyfiles": "^2.4.1",
    "cpx": "^1.5.0",
    "eslint": "^8.32.0"
  }
}
